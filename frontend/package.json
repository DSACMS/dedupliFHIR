{
  "name": "deduplifhir-desktop-app",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "lint": "eslint ./**.js",
    "lint:fix": "eslint --quiet --fix \"**.js\"",
    "prettier": "prettier --check ./**.{js,css}",
    "prettier:fix": "prettier --write \"./**.{js,css}\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "pack": "electron-builder --dir",
    "dist": "../set-up-python-env.sh && electron-builder"
  },
  "author": "",
  "license": "ISC",
  "build": {
    "appId": "com.example.app",
    "extraResources": [
      {
        "from": "../python",
        "to": "python",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "../cli",
        "to": "cli",
        "filter": [
          "**/*"
        ]
      }
    ]
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "electron": "^28.0.0",
    "electron-builder": "^24.13.3",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5"
  },
  "dependencies": {
    "@uswds/uswds": "3.7.1",
    "fs-extra": "^11.2.0",
    "python-shell": "^5.0.0"
  }
}
